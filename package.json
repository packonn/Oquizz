{
    "name": "quizz",
    "version": "1.0.0",
    "description": "Application de quizz",
    "main": "server.js",
    "scripts": {
        "test": "node serveur.js",
        "resetDB": "psql -U oquiz -d oquiz -f ./app/data/create_tables.sql",
        "addDB": "psql -U oquiz -d oquiz -f ./app/data/populate_tables.sql"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "prettier": "^3.1.1"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "overrides": [
            {
                "files": "*.js",
                "options": {
                    "parser": "babel"
                }
            },
            {
                "files": "*.json",
                "options": {
                    "parser": "json",
                    "tabWidth": 4
                }
            }
        ],
        "semi": true,
        "requirePragma": false,
        "proseWrap": "preserve",
        "arrowParens": "avoid",
        "htmlWhitespaceSensitivity": "css",
        "quoteProps": "as-needed"
    }
}
